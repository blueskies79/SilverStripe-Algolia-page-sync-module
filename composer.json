{
    "name": "directlease/silverstripe-algolia-page-sync-module",
    "description": "A module that based on configuration syncs pages to an Algolia index",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "algolia"
    ],
    "license": "MIT",
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "algolia/algoliasearch-client-php": "^2.7"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DirectLease\\AlgoliaPageSyncModule\\": "src/"
        }
    },
    "extra": {
        "installer-name": "algolia-page-sync-module"
    }
}
